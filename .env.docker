# Database Settings (configured for Docker)
MONGO_ROOT_USERNAME=admin
MONGO_ROOT_PASSWORD=secure_admin_password_123
MONGODB_URI=mongodb://${MONGO_ROOT_USERNAME}:${MONGO_ROOT_PASSWORD}@mongodb:27017/eternals-rebirth?authSource=admin